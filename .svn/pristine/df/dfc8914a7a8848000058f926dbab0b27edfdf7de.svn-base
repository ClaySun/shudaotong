/*--------图片悬停-----------*/
.tv_hot {
    width: 228px; background: #fff; padding: 5px; border: 1px solid #efefef;
    position: relative;
}
.tv_hot .sc-sm{ color: #fff; z-index: 9999; position: absolute; top:60px; left:100px ; border: 1px solid #fff; padding: 5px 10px;}
.avatar {
    position: relative;
}
.tv_hot .avatar {
    display: block;
    height: 150px;
    overflow: hidden;
    width: 228px;
}
.tv_hot .avatar img {
    height: 150px;
}
.avatar:after {
    bottom: 0;
    content: url("../images/bg_play.png");
    left: 5px;
    position: absolute;
    z-index: 10;
}
.avatar:hover, .zhuti .avatar:hover, .bbsinner .avatar:hover {
   
}
.avatar:hover img, .zhuti .avatar:hover img, .bbsinner .avatar:hover img {
    transform: scale(1.3);
    transition: all 1s ease 0s;
	-webkit-transform: scale(1.3);
	-webkit-transform: all 1s ease 0s;
}

/*------常见问题------*/
#accmenu {
    float: left;
    width: 166px;
    background: #fff;
    margin: 0 0 55px;
    border: 1px solid #e1e1e1;
}
#accmenu h3 {
    font-size: 24px;
    font-weight: normal;
    color: #fff;
    height: 64px;
    line-height: 64px;
    background:#296f1f;
    text-align: center;
}
ul, li {
    list-style-type: none;
}
#accmenu ul li {
    height: 54px;
    line-height: 54px;
    font-size: 16px;
    border-top: 1px solid #f3f3f3;
}
#accmenu ul li.active a {
    color: #296f1f;
}
#accmenu ul li a {
    display: block;
    padding: 0 0 0 45px;
    position: relative;
    color: #555;
}
#accmenu ul li.active a::after {
    content: '';
    display: block;
    border-width: 4px 0 4px 7px;
    border-style: solid;
    border-color: transparent transparent transparent #ff5c54;
    position: absolute;
    right: 40px;
    top: 50%;
    margin-top: -4px;
}
#basemain {
    float: left;
    width: 960px;
    min-height: 200px;
    /*min-height: 394px;*/
    background: #fff;
    margin: 0 0 55px;
    padding: 0 25px 35px 25px;
    border: 1px solid #e1e1e1;
    line-height: 1.8;
    color: #555;
    margin-left: 20px;
}
