/*nav*/
.nav { width: 100%;
	height: 40px;
	background-color: #3b3a3d;
	font-size: 16px;
	/*background-color: #f51816;*/
}
.nav-content {
	width: 1200px;
	margin-left: auto;
	margin-right: auto;
}
.nav-content dt span a{ font-size: 12px; float: right; margin-right: 10px; color: #fff;}
.nav-content>dt,
.nav-content>dd {
	float: left;
	height: 40px;
	line-height: 40px;
	color: #fff;
}
.nav-content>dd>a { color: #fff; font-size:16px;}
.nav-content>dt { 	
	width: 190px;
   padding-left:10px ;
	font-weight: bold;
	background: #167608;
	font-size:16px;
}
.nav-content>dd { 	
	padding-left: 20px;
	padding-right: 20px;
}
.nav-content>dd:hover { background-color: #296f1f;}

